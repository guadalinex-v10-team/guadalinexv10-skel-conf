# Hack gnome-accessibility-profiles

# Copy /usr/share/a11y-profile-manager/profiles/gnome-* into /usr/share/a11y-profile-manager/profiles/* 
for i in /usr/share/a11y-profile-manager/profiles/gnome-*
do 
  gnome_profile=$(basename $i)
  profile=$(expr substr $gnome_profile 7 100)
  
  if [  ! -d /usr/share/a11y-profile-manager/profiles/$profile ]
  then
     cp -r /usr/share/a11y-profile-manager/profiles/$gnome_profile /usr/share/a11y-profile-manager/profiles/$profile
  fi  
  
done

